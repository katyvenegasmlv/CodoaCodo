<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Checklogin</servlet-name>
        <servlet-class>newpackage.Checklogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IngresarPedido</servlet-name>
        <servlet-class>newpackage.IngresarPedido</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertNewUser</servlet-name>
        <servlet-class>newpackage.InsertNewUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConfirmarCompra</servlet-name>
        <servlet-class>newpackage.ConfirmarCompra</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModificarDatos</servlet-name>
        <servlet-class>newpackage.ModificarDatos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminarDatos</servlet-name>
        <servlet-class>newpackage.EliminarDatos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModificarPedido</servlet-name>
        <servlet-class>newpackage.ModificarPedido</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Checklogin</servlet-name>
        <url-pattern>/Checklogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IngresarPedido</servlet-name>
        <url-pattern>/IngresarPedido</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertNewUser</servlet-name>
        <url-pattern>/InsertNewUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConfirmarCompra</servlet-name>
        <url-pattern>/ConfirmarCompra</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModificarDatos</servlet-name>
        <url-pattern>/ModificarDatos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminarDatos</servlet-name>
        <url-pattern>/EliminarDatos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModificarPedido</servlet-name>
        <url-pattern>/ModificarPedido</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
